<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="enties.ExcellentStar" table="excellent_star">
        <id name="id" type="java.lang.Integer">
            <column name="id"></column>
            <generator class="native"></generator>
        </id>
        <property name="graName" type="java.lang.String">
            <column name="gra_name"></column>
        </property>
        <property name="graGrade" type="java.lang.String">
            <column name="gra_grade"></column>
        </property>
        <property name="graPosion" type="java.lang.String">
            <column name="gra_posion"></column>
        </property>
        <property name="graPlace" type="java.lang.String">
            <column name="gra_place"></column>
        </property>
    </class>




</hibernate-mapping>