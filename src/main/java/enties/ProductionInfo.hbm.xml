<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="enties.ProductionInfo" table="production_info">
        <id name="id" type="java.lang.Integer">
            <column name="id"></column>
            <generator class="native"></generator>
        </id>
        <property name="proName" type="java.lang.String">
            <column name="pro_name"></column>
        </property>
        <property name="proImgUrl" type="java.lang.String">
            <column name="pro_img_url"></column>
        </property>
        <property name="proInfo" type="java.lang.String">
            <column name="pro_info"></column>
        </property>
    </class>
</hibernate-mapping>