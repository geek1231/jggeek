<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="enties.MemberInfo" table="member_info">
    <id name="id" type="java.lang.Integer">
        <column name="id">
        </column>
        <generator class="native"></generator>
    </id>
    <property name="position" type="java.lang.String">
        <column name="position"></column>
    </property>
    <property name="personPosition" type="java.lang.String">
        <column name="person_position"></column>
    </property>
    <property name="personName" type="java.lang.String">
        <column name="person_name"></column>
    </property>
    <property name="personInfo" type="java.lang.String">
        <column name="person_info"></column>
    </property>
    <property name="personImgUrl" type="java.lang.String">
        <column name="person_img_url"></column>
    </property>
</class>

</hibernate-mapping>